<h2>Hi! <%= @agent.name %></h2>
<h3>Your Region: <%= link_to @agent.region.name, region_path(@agent.region) %></h3>
<p><%= link_to "Create Lead", new_lead_path %> <%=   link_to "Your Leads", leads_path %></p>
<% if @agent.manager %>
  <p><%= link_to "Leads in Your Region", region_path(@agent.region) %><%=   link_to "Your Agents", agents_path %></p>
<% end %>

<table>
  <tr>
    <th>Total Leads</th>
    <th>Total Go</th>
    <th>Total No Go</th>
    <th>Total Converted</th>
    <th>Total Booked Loans</th>
  </tr>
  <tr>
    <td><%= @total_leads %></td>
    <td><%= @total_go %></td>
    <td><%= @total_nogo %></td>
    <td><%= @total_converted %></td>
    <td><%= @total_booked %></td>
  </tr>
</table>

<p><%= form_tag signout_path do %>
  <input type=submit value='Sign Out'>
<% end %>
</p>
