<h1><%= @agent.name %></h1>
<h2><%= @agent.region.name %></h2>

<table>
  <tr>
    <th>ID no</th>
    <th>Industry</th>
    <th>Lead Name</th>
    <th>Status</th>
    <th>Booked Amount</th>
    <th>
  </tr>
  <% @agent.leads.each do |lead| %>
  <tr>
      <td><%= lead.id %></td>
      <td><%= lead.industry.name %></td>
      <td><%= lead.name %></td>
      <td><%= lead.status %></td>
      <td><%= lead.booked_loans %></td>
  </tr>
  <% end %>
</table>
